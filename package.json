{
  "name": "genai",
  "displayName": "genAI",
  "description": "genAI is a VS Code extension designed to streamline the process of managing connections to a server, creating and editing assertions, running tests, and generating test reports. It provides a user-friendly interface within VS Code for developers working on projects that require server interaction and automated testing",
  "version": "0.3.0",
  "publisher": "GAIB",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "genai.login",
        "title": "GAIB: Login"
      },
      {
        "command": "genai.runTests",
        "title": "GAIB: Run Tests"
      },
      {
        "command": "genai.editAssertions",
        "title": "GAIB: Edit Assertions"
      },
      {
        "command": "genai.generateAssertions",
        "title": "GAIB: Generate Assertions"
      },
      {
        "command": "genai.viewReport",
        "title": "GAIB: View Report"
      },
      {
        "command": "genai.connectGit",
        "title": "GAIB: Connect Git"
      },
      {
        "command": "genai.updateAssertions",
        "title": "GAIB: Update Assertions"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.87.0",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "mongodb": "^6.5.0",
    "octokit": "^3.2.0",
    "simple-git": "^3.24.0"
  }
}
